{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "Restaurant"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
